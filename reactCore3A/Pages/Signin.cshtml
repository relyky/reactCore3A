@page
@model reactCore3A.Pages.SigninModel
@{
    ViewData["Title"] = "Signin";
}

<h1>Signin</h1>

@if (ViewData["Message"] != null)
{
    <p>
        @ViewData["Message"]
    </p>
}

<form method="post">
    userId:
    <input asp-for="LoginInfo.userId" />
    credential:
    <input asp-for="LoginInfo.credential" />
    <input type="submit" />
</form>
